<h2 class="title">
  <%= page.name %>
</h2>
<div class="row">
  <div class="col-md-6">
   <p class="description">
     <%= page.indents.first.body %>
    </p>
  </div>
</div>
<div class="row">
  <% page.indents.offset(1).each do |item|%>
  <div class="col-md-4 indent-box">
       
      <h4 class="indent-title">
        <%= item.title %>
      </h4>
      <p class="description">
        <%= item.body %>
      </p>
  </div>
  <% end %>
</div>